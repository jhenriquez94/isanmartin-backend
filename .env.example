# Configuración de la base de datos MySQL
DB_HOST=localhost
DB_PORT=3306
DB_USER=usuario_db
DB_PASSWORD=contraseña_db
DB_NAME=nombre_db

# Configuración de JWT
JWT_SECRET=clave_jwt_secreta_larga_y_segura

# URL del frontend
FRONTEND_URL=https://nombre_db.cl
FRONTEND_PRODUCTION_URL=https://nombre_db.cl

# Puerto del servidor (normalmente 3000 para shared hosting)
PORT=3000

# Entorno
NODE_ENV=production 